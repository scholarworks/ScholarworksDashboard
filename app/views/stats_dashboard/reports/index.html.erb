<div id="page-wrapper">
     <div class="row">
         <div class="col-lg-12">
             <h1 class="page-header">Reports</h1>
         </div>
         <!-- /.col-lg-12 -->
     </div>
     <!-- /.row -->
     <div class="row">
       <div class="col-lg-6">
                           <div class="panel panel-default">
                               <div class="panel-heading">
                                   Top 10 Non-Crawler Hosts
                               </div>
                               <!-- /.panel-heading -->
                               <div class="panel-body">
                                   <div class="table-responsive">
                                       <table class="table table-striped table-bordered table-hover">
                                           <thead>
                                               <tr>
                                                   <th>IP Address</th>
                                                   <th>Host Name</th>
                                                   <th>Visits</th>
                                               </tr>
                                           </thead>
                                           <tbody>
                                           <% @top_10_non_crawlers.top_10_non_crawlers.each do |host| %>
                                               <tr>
                                                   <td><%= host[0] %></td>
                                                   <td><%= host[1] %></td>
                                                   <td><%= host[2] %></td>
                                               </tr>
                                           <% end %>
                                           </tbody>
                                       </table>
                                   </div>
                                   <!-- /.table-responsive -->
                               </div>
                               <!-- /.panel-body -->
                           </div>
                           <!-- /.panel -->
                       </div>
                       <!-- /.col-lg-6 -->
                       <div class="col-lg-6">
                                           <div class="panel panel-default">
                                               <div class="panel-heading">
                                                   Top 10 Crawler Hosts
                                               </div>
                                               <!-- /.panel-heading -->
                                               <div class="panel-body">
                                                   <div class="table-responsive">
                                                       <table class="table table-striped table-bordered table-hover">
                                                           <thead>
                                                               <tr>
                                                                   <th>IP Address</th>
                                                                   <th>Host Name</th>
                                                                   <th>Visits</th>
                                                               </tr>
                                                           </thead>
                                                           <tbody>
                                                           <% @top_10_non_crawlers.top_10_crawlers.each do |host| %>
                                                               <tr>
                                                                   <td><%= host[0] %></td>
                                                                   <td><%= host[1] %></td>
                                                                   <td><%= host[2] %></td>
                                                               </tr>
                                                           <% end %>
                                                           </tbody>
                                                       </table>
                                                   </div>
                                                   <!-- /.table-responsive -->
                                               </div>
                                               <!-- /.panel-body -->
                                           </div>
                                           <!-- /.panel -->
                                       </div>
                                       <!-- /.col-lg-6 -->

</div>
</div>
